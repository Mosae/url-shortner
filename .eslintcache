t\t}\n\t\t\t})\n\t\t\t.catch(() => {\n\t\t\t\tconsole.log('error getting the info');\n\t\t\t\tsetError(true);\n\t\t\t});\n\t};\n\tconst handleChange = (e) => {\n\t\tsetCurrentLink(e.target.value);\n\t};\n\n\treturn (\n\t\t<section className=\"shorten\">\n\t\t\t<div className=\"search__container\">\n\t\t\t\t<form action=\"\" onSubmit={handleSubmit} className={error ? 'error' : ''} onFocus={() => setError(false)}>\n\t\t\t\t\t<img src={shortenDesktop} alt=\"background\" className=\"shorten-desktop-BG\" aria-hidden=\"true\" />\n\t\t\t\t\t<div className=\"shorten__input\">\n\t\t\t\t\t\t<input type=\"text\" placeholder=\"Shorten a link here...\" value={currentLink} onChange={handleChange} aria-label=\"shorten-link\" />\n\t\t\t\t\t\t<p className=\"error-message\">Please add a link</p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<input type=\"submit\" className=\"secondary_it_button\" value=\"Shorten It!\" />\n\t\t\t\t</form>\n\t\t\t\t{links && links.map((link, i) => <Result link={link} id={i} />)}\n\t\t\t</div>\n\t\t</section>\n\t);\n}\n\nconst Result = ({ link, i }) => {\n\tfunction copyToClipboard(e) {\n\t\tnavigator.clipboard.writeText(link['shortLink']);\n\t\te.target.focus();\n\t}\n\treturn (\n\t\t<div className=\"return_result\" key={i}>\n\t\t\t<h3 className=\"result__heading\">{link['shortLink']}</h3>\n\t\t\t<div className=\"result\">\n\t\t\t\t<a href={link['shortLink']} className=\"result__url\"></a>\n\t\t\t\t<button id=\"btn\" className=\"secondary-btn\" onClick={copyToClipboard}>\n\t\t\t\t\t<span className=\"copy\">Copy</span>\n\t\t\t\t\t<span className=\"copied\">Copied!</span>\n\t\t\t\t</button>\n\t\t\t</div>\n\t\t</div>\n\t);\n};\n\nexport default Search;\n",["57","58"],"/Users/mosaelitsoane/Desktop/Side-Projects/url-shortening-api-master/url-short/src/components/Footer/Footer.js",[],"/Users/mosaelitsoane/Desktop/Side-Projects/url-shortening-api-master/url-short/src/components/MobileMenu/MobileMenu.js",[],{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"